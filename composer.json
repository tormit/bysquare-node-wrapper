{
  "name": "tormit/bysquare-node-wrapper",
  "description": "PHP Wrapper for https://www.npmjs.com/package/bysquare package",
  "license": [
    "BSD-3-Clause",
    "GPL-2.0",
    "GPL-3.0"
  ],
  "keywords": [
    "bysquare",
    "paybysquare"
  ],
  "require": {
    "php": "^7.4 || ^8.0",
    "ext-gd": "*",
    "ext-json": "*",
    "composer/composer": "^2.0.0",
    "endroid/qr-code": "~4.5.2",
    "symfony/console": "^5.1 || ^6.0",
    "symfony/filesystem": "^5.1 || ^6.0",
    "symfony/process": "^5.1 || ^6.0"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.28",
    "phpunit/phpunit": "^9.0.0"
  },
  "autoload": {
    "psr-4": {
      "Tormit\\BysquareNodeWrapper\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "ergebnis/composer-normalize": true
    }
  }
}
